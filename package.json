{
  "name": "aicodehive-hello-world",
  "displayName": "AI Code Hive: Desenvolvimento Agile",
  "description": "Uma extensão para desenvolvimento ágil e produtivo",
  "version": "1.0.0",
  "publisher": "aicodehive",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "hello",
    "world",
    "demo"
  ],
  "activationEvents": [
    "onCommand:aicodehive.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aicodehive.helloWorld",
        "title": "Desenvolvimento Agile",
        "category": "AICodeHive"
      }
    ],
    "configuration": {
      "title": "AI Code Hive",
      "properties": {
        "aiCodeHive.stackspot.clientId": {
          "type": "string",
          "default": "",
          "description": "StackSpot Client ID para autenticação"
        },
        "aiCodeHive.stackspot.clientSecret": {
          "type": "string",
          "default": "",
          "description": "StackSpot Client Secret para autenticação"
        },
        "aiCodeHive.stackspot.slugs.createStories": {
          "type": "string",
          "default": "",
          "description": "SLUG para criação de histórias"
        },
        "aiCodeHive.stackspot.slugs.detailBusiness": {
          "type": "string",
          "default": "",
          "description": "SLUG para detalhamento de negócio"
        },
        "aiCodeHive.stackspot.slugs.detailTechnical": {
          "type": "string",
          "default": "",
          "description": "SLUG para detalhamento técnico"
        },
        "aiCodeHive.stackspot.slugs.createTests": {
          "type": "string",
          "default": "",
          "description": "SLUG para criação de testes"
        },
        "aiCodeHive.stackspot.slugs.createTasks": {
          "type": "string",
          "default": "",
          "description": "SLUG para criação de tarefas"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  }
}